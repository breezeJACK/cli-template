{
  "name": "my-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --host",
    "build:staging": "vite build --mode staging",
    "build": "vite build",
    "serve": "vite preview",
    "eslint": "npx eslint . --ext .vue,.tsx,.js,.ts,.tsx, --fix src",
    "prettier": "prettier --write '**/**.{js,jsx,.vue,tsx,ts}'",
    "windicss": "npx windicss-analysis",
    "test": "vitest",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@vueuse/core": "^7.7.0",
    "ant-design-vue": "^3.0.0-beta.13",
    "axios": "^0.26.0",
    "dayjs": "^1.10.8",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.11",
    "query-string": "^7.1.1",
    "vue": "^3.2.31",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@sentry/browser": "^6.18.1",
    "@sentry/integrations": "^6.18.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "@vue/test-utils": "^2.0.0-rc.17",
    "consola": "^2.15.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.10.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "less": "^4.1.2",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "typescript": "4.5.2",
    "unplugin-auto-import": "^0.6.1",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.8.6",
    "vite-plugin-inspect": "^0.4.3",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-windicss": "^1.8.2",
    "vitest": "^0.5.9",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.32.1"
  }
}